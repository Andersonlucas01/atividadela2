const readline = require('readline');

const rl = readline.createInterface({
  input: process.stdin,
  output: process.stdout
});

rl.question('Digite o n-Ã©simo termo: ', (n) => {
  let a = 0;
  let b = 1;
  let i = 0;

  while (i < n) {
    console.log(a);
    let temp = a;
    a = b;
    b = temp + b;
    i++;
  }

  rl.close();
});
